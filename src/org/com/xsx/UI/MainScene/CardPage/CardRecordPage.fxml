<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="765.0" prefWidth="1240.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0">
               <children>
                  <VBox alignment="CENTER" spacing="20.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="20.0">
                     <children>
                        <VBox alignment="CENTER" spacing="15.0">
                           <children>
                              <Label text="库存图">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <GridPane>
                                 <children>
                                    <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
                                       <children>
                                          <Button mnemonicParsing="false" onAction="#GB_QueryAction" prefHeight="35.0" prefWidth="70.0" style="-fx-background-color: #8080C0;" text="查询" />
                                       </children>
                                       <padding>
                                          <Insets left="20.0" />
                                       </padding>
                                    </HBox>
                                    <GridPane vgap="20.0">
                                       <children>
                                          <Label text="项目" />
                                          <Label text="设备" GridPane.rowIndex="1" />
                                          <FlowPane fx:id="GB_ItemFlowPane" hgap="10.0" GridPane.columnIndex="1" />
                                          <FlowPane fx:id="GB_DeviceFlowPane" hgap="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                       </children>
                                       <columnConstraints>
                                          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
                                          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                       </columnConstraints>
                                       <rowConstraints>
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                       </rowConstraints>
                                    </GridPane>
                                 </children>
                                 <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
                                 </columnConstraints>
                                 <rowConstraints>
                                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                 </rowConstraints>
                                 <VBox.margin>
                                    <Insets left="20.0" right="20.0" />
                                 </VBox.margin>
                              </GridPane>
                              <LineChart fx:id="GB_CardLineChart" prefHeight="500.0">
                                <xAxis>
                                  <CategoryAxis fx:id="GB_CardLineXAxis" side="BOTTOM" />
                                </xAxis>
                                <yAxis>
                                  <NumberAxis fx:id="GB_CardLineYAxis" side="LEFT" />
                                </yAxis>
                              </LineChart>
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" spacing="5.0">
                           <children>
                              <Label text="出入库记录">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <TableView fx:id="GB_CardTableView" prefHeight="600.0">
                                <columns>
                                  <TableColumn fx:id="GB_TableColumn1" prefWidth="75.0" text="索引" />
                                  <TableColumn fx:id="GB_TableColumn2" prefWidth="75.0" text="项目" />
                                    <TableColumn fx:id="GB_TableColumn3" prefWidth="75.0" text="数目" />
                                    <TableColumn fx:id="GB_TableColumn4" prefWidth="75.0" text="时间" />
                                    <TableColumn fx:id="GB_TableColumn5" prefWidth="75.0" text="操作人" />
                                    <TableColumn fx:id="GB_TableColumn6" prefWidth="75.0" text="领料人" />
                                    <TableColumn fx:id="GB_TableColumn7" prefWidth="75.0" text="领料设备" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <Pagination fx:id="GB_Pagination" maxPageIndicatorCount="20" styleClass="bullet" />
                           </children>
                        </VBox>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <StackPane fx:id="GB_FreshPane" prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Region prefHeight="696.0" prefWidth="1200.0" style="-fx-background-color: rgba(0, 0, 0, 0.4);" />
            <ProgressIndicator fx:id="GB_RefreshBar" maxHeight="150.0" maxWidth="150.0" prefHeight="120.0" prefWidth="120.0" progress="0.0" />
         </children>
      </StackPane>
   </children>
</AnchorPane>
